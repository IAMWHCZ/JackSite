# 开发环境配置
VITE_APP_ENV=development
VITE_APP_NAME=JackSite Authentication System (Dev)
VITE_APP_VERSION=1.0.0

# API 配置
VITE_API_BASE_URL=http://localhost:6608/api
VITE_API_TIMEOUT=10000

# 认证配置
VITE_AUTH_TOKEN_KEY=jack_auth_token
VITE_AUTH_REFRESH_TOKEN_KEY=jack_refresh_token
VITE_AUTH_TOKEN_EXPIRES_IN=3600000
VITE_AUTH_COOKIE_DOMAIN=localhost

# 第三方登录配置
VITE_GOOGLE_CLIENT_ID=your_google_client_id_dev
VITE_GITHUB_CLIENT_ID=your_github_client_id_dev

# 调试配置
VITE_DEBUG_MODE=true
VITE_SHOW_DEVTOOLS=true
VITE_LOG_LEVEL=debug

# 国际化配置
VITE_DEFAULT_LANGUAGE=zh-CN
VITE_FALLBACK_LANGUAGE=en-US

# 主题配置
VITE_DEFAULT_THEME=system
VITE_THEME_STORAGE_KEY=jack_theme_preference

# 功能开关
VITE_ENABLE_PWA=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true