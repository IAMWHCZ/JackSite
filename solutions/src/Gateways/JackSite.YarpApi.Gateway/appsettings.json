{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsPolicy": {
    "Origins": [
      "http://localhost:3000",
      "https://localhost:3000",
      "http://localhost:5173",
      "https://localhost:5173",
      "http://localhost:8000",
      "https://localhost:8000",
      "https://jackcheng.cn"
    ],
    "Methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS",
      "PATCH",
      "HEAD"
    ],
    "Headers": [
      "Accept",
      "Content-Type",
      "Origin",
      "X-Requested-With",
      "Authorization",
      "X-CSRF-Token",
      "X-Client-Id",
      "X-Client-Version"
    ],
    "ExposedHeaders": [
      "Content-Disposition",
      "X-Pagination",
      "X-Total-Count"
    ],
    "AllowCredentials": true
  },
  "ReverseProxy": {
    "Routes": {
      "frontend-route": {
        "ClusterId": "frontend-cluster",
        "Match": {
          "Path": "/frontend/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "identity-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/identity-api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "monitor-route": {
        "ClusterId": "monitor-cluster",
        "Match": {
          "Path": "/monitor-api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "user-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/user-api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "webtools-route": {
        "ClusterId": "webtools-cluster",
        "Match": {
          "Path": "/webtools-api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "frontend-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5173"
          }
        }
      },
      "identity-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://jacksite.identity.host:80"
          }
        }
      },
      "monitor-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://jacksite.monitorserver:80"
          }
        }
      },
      "user-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://jacksite.userserver:80"
          }
        }
      },
      "webtools-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://jacksite.webtools.server:80"
          }
        }
      }
    }
  },
  "Scalar": {
    "Title": "JackSite.YarpApi.Gateway"
  },
  "ApplicationDomain": {
